{
  "name": "yeogiya-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 8000 --open",
    "build": "vite build --base=./",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preinstall": "npx only-allow pnpm",
    "prepare-husky": "chmod +x .husky/commit-msg && husky install",
    "postinstall": "if [ -z \"$VERCEL\" ]; then pnpm run prepare-husky; else echo 'Skipping Husky setup on Vercel'; fi",
    "test": "jest src"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@storybook/client-api": "^7.6.6",
    "@tanstack/react-query": "^5.14.6",
    "@tanstack/react-query-devtools": "^5.14.6",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-calendar": "^4.7.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.1",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@hookform/devtools": "^4.3.1",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-vite": "^7.6.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.2",
    "redux-persist": "^6.0.0",
    "storybook": "^7.6.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
